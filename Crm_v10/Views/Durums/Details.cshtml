@model Crm_v10.Models.Durum

@{
    ViewBag.Title = "Details";
}

<div class="container">
    <div class="row col-sm-10">
        <h2>Durum</h2>
        <div class="col-sm-12">
            <ol class="breadcrumb">
                <li>
                    <a href="#">Kartlar</a>
                </li>
                <li>
                    <a href="/Durums/Index">Durum Tanıtım Kartı</a>
                </li>
                <li>
                    <a href="#">Durum Bilgilerini İncele</a>
                </li>
            </ol>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-6">
            <div class="card-box">
                <h2>İncele</h2>
                <h4 class="m-t-0 header-title">
                    <b>DURUM BİLGİLERİNİ İNCELE</b>
                </h4>
                <div class="form-horizontal">
                    <hr />

                    <dl class="dl-horizontal">
                        <dt>
                           Durum Adı :
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.DurumAdi)
                        </dd>
                    </dl>
                </div>
                @Html.ActionLink("Listeye Dön", "Index")
            </div>
        </div>
    </div>
</div>

