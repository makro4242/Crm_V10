@model Crm_v10.Models.SatisElemanlari

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-lg-4">
            <div class="card-box">
                <h2>İncele</h2>
                <h4 class="m-t-0 header-title">
                    <b>SATIŞ ELEMANI BİLGİLERİNİ İNCELE</b>
                </h4>
                <div class="form-horizontal">
                    <hr />
                    <dl class="dl-horizontal">
                        <dt>
                           Satış Elemanı Kodu :
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.SatisElemaniKodu)
                        </dd>

                        <dt>
                           Ad Soyad :
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.SatisElemaniAdiSoyadi)
                        </dd>

                    </dl>
                </div>
            </div>
            @Html.ActionLink("Listeye Dön", "Index")
        </div>
    </div>
</div>
