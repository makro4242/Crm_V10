// Yetkili Kodu Potansiyel Kodu gibi alanlarda ver,tabanındaki en son kayıt kodunu alıp 1 ekleyip kodu alanına geri döndürür
        $(document).ready(function () {
            $(".koduTamamla").keydown(function (e) {
                if ((e.which || e.keyCode) == 112) {
                    var kod = document.querySelector('.koduTamamla').value;
                    var sayfa = document.getElementById("sayfa").value;
                    $.ajax({
                        type: "POST",
                        url: "/" + sayfa + "/KoduGetir",
                        data: "{kod:'" + kod + "'}",
                        contentType: "application/json; charset=utf-8",
                        success: function (data) {
                            if (data == "") {

                            }
                            else { document.querySelector('.koduTamamla').value = data; }
                        },
                        error: function () {
                            swal('Hata', 'error');
                        }
                    });
                }
            });
        })
    </script>
