    <script>
        $('input.' + "tarih").keypress(function (event) { //Tarihlere Sadece sayı ve '/' karakterinin girilmesini saglar
            var key = window.event ? event.keyCode : event.which;
            if (event.keyCode === 8 || event.keyCode === 46) {
                return true;
            } else if (key < 47 || key > 57) {
                return false;
            } else {
                return true;
            }
        })
    </script>
    <script>
        $(document).ready(function () {
            $('.Tel1').change(function () {
                var number = document.querySelector('.Tel1').value;
                number = number.replace(/[()-]/g, '');
                number = number.replace(' ', '');
                var formatted = number.substr(0, 1) + "(" + number.substr(1, 3) + ') ' + number.substr(4, 3) + '-' + number.substr(7, 4);
                $(".Tel1").val(formatted);
            });
            $('.Tel2').change(function () {
                var number = document.querySelector('.Tel2').value;
                number = number.replace(/[()-]/g, '');
                number = number.replace(' ', '');
                var formatted = number.substr(0, 1) + "(" + number.substr(1, 3) + ') ' + number.substr(4, 3) + '-' + number.substr(7, 4);
                $(".Tel2").val(formatted);
            });
        });
    </script>
    <script>
        $(document).ready(function () {
            $('.para').change(function () {
                var number = document.querySelector('.para').value,
                    dollars = number.split(',')[0],
                    cents = (number.split(',')[1] || '') + '00';
                dollars = dollars.split('').reverse().join('')
                    .replace(/(\d{3}(?!$))/g, '$1.')
                    .split('').reverse().join('');
                $(".para").val(dollars + ',' + cents.slice(0, 2));
                document.querySelector('.gosterilmeyenparaformati').value = number;
            });


        });
    </script>
    <script>
        $('input.' + "sayi").keypress(function (event) { // Sadece sayı ve '/,.' karakterinin girilmesini saglar
            var key = window.event ? event.keyCode : event.which;
